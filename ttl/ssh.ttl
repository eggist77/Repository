target='192.168.0.1'
username='user'
password='password'

; ssh
strconcat target ':22 /ssh /2 /auth=password /user='
strconcat target username
strconcat target ' /passwd='
strconcat target password
connect target

; 5sec pause
pause 5

; logout
sendln 'exit'

end

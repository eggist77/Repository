terget='192.168.0.1'
username='user'
password='password'

; telnet
strconcat terget ':23 /nossh /T=1'
connect terget

; login
wait 'login:'
sendln username

wait 'Password:'
sendln password

; 5sec pause
pause 5

; logout
sendln 'exit'

end
